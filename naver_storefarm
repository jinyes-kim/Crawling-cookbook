# naver store farm

from bs4 import BeautifulSoup
from selenium import webdriver

driver = webdriver.Chrome("C:/Chromedriver")
driver.get(url)

# Xpath 계속 변하는 듯
for a in range(2, 12):
    driver.find_element_by_xpath('//*[@id="content"]/div[16]/div[3]/div/div[2]/a[' + str(a) + ']').click()
    driver.get("url")
    html = driver.page_source
    soup = BeautifulSoup(html, "html.parser")
    dummy = soup.select("._1iaDS5tcmC")
    print(dummy)

